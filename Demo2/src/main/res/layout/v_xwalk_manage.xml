<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/margin_default">

    <com.hy.demo2.widget.NumberProgressBar
        android:id="@+id/numberbar3"
        style="@style/NumberProgressBar_Funny_Orange"
        app:progress_current="30"
        app:progress_reached_bar_height="10dp"
        app:progress_text_size="18sp"
        app:progress_unreached_bar_height="1dp" />

    <Button
        android:id="@+id/xwalk_manage_btnDownload"
        style="@style/BtnStyle"
        android:focusable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name" >
        <requestFocus/>
    </Button>
</LinearLayout>