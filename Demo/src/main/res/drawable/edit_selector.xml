<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/edit_disable" />
            <stroke android:width="1px" android:color="@color/edit_border" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/white" />
            <stroke android:width="1px" android:color="@color/edit_border_ed" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white" />
            <stroke android:width="1px" android:color="@color/edit_border" />
        </shape>
    </item>
</selector>